<!DOCTYPE html>
<html lang="en">

<%- include('partials/header') %>

    <head>
        <link rel="stylesheet" href="/css/meet-the-team.css">
    </head>

    <body id="page-container">
        <div id="content-wrap">
            <%- include('partials/navbar') %>
                <% for(var i=0; i < 25; i++) { %>
                    <div class="modal fade" id="modal<%=i%>" tabindex="-1" aria-labelledby="exampleModalLabel"
                        aria-hidden="true">
                        <div class="modal-dialog modal-dialog-centered modal-lg">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                </div>
                                <div class="modal-body">
                                    <div class="d-flex flex-row flex-wrap">
                                        <div class="col-8">
                                            <%= jsonContent[i].bio %>
                                        </div>
                                        <div class="col-4">
                                            <img src="<%= jsonContent[i].picture %>">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <% } %>

                        <div class="container d-flex flex-wrap body-full title-header">
                            <div class="main-title">
                                <h1>Meet The <span>Team!</span></h1>
                            </div>
                            <div class="d-flex flex-row flex-wrap cochairs">
                                <div class="col-3"></div>
                                <div class="col-3">
                                    <button type="button" class="btn" data-toggle="modal" data-target="#modal0">
                                        <img src="<%= jsonContent[0].picture %>">
                                    </button>
                                    <h2>
                                        <%= jsonContent[0].firstName %> <br />
                                            <%= jsonContent[0].lastName %>
                                    </h2>
                                </div>
                                <div class="col-3">
                                    <button type="button" class="btn" data-toggle="modal" data-target="#modal1">
                                        <img src="<%= jsonContent[1].picture %>">
                                    </button>
                                    <h2>
                                        <%= jsonContent[1].firstName %> <br />
                                            <%= jsonContent[1].lastName %>
                                    </h2>
                                </div>
                                <div class="col-3"></div>
                            </div>
                            <div class="d-flex flex-row flex-wrap rows">
                                <div class="col">
                                    <% for(var i=2; i < 8; i++) { %>
                                        <button type="button" class="btn" data-toggle="modal"
                                            data-target="#modal<%=i%>">
                                            <img src="<%= jsonContent[i].picture %>">
                                        </button>
                                        <h2>
                                            <%= jsonContent[i].firstName %> <br />
                                                <%= jsonContent[i].lastName %>
                                        </h2>
                                        <% } %>
                                </div>
                                <div class="col">
                                    <% for(var i=8; i < 14; i++) { %>
                                        <button type="button" class="btn" data-toggle="modal"
                                            data-target="#modal<%=i%>">
                                            <img src="<%= jsonContent[i].picture %>">
                                        </button>
                                        <h2>
                                            <%= jsonContent[i].firstName %> <br />
                                                <%= jsonContent[i].lastName %>
                                        </h2>
                                        <% } %>
                                </div>
                                <div class="col">
                                    <% for(var i=14; i < 20; i++) { %>
                                        <button type="button" class="btn" data-toggle="modal"
                                            data-target="#modal<%=i%>">
                                            <img src="<%= jsonContent[i].picture %>">
                                        </button>
                                        <h2>
                                            <%= jsonContent[i].firstName %> <br />
                                                <%= jsonContent[i].lastName %>
                                        </h2>
                                        <% } %>
                                </div>
                                <div class="col">
                                    <% for(var i=20; i < 25; i++) { %>
                                        <button type="button" class="btn" data-toggle="modal"
                                            data-target="#modal<%=i%>">
                                            <img src="<%= jsonContent[i].picture %>">
                                        </button>
                                        <h2>
                                            <%= jsonContent[i].firstName %> <br />
                                                <%= jsonContent[i].lastName %>
                                        </h2>
                                        <% } %>
                                </div>
                            </div>
                        </div>
        </div>
        <%- include('partials/footer') %>

    </body>

</html>